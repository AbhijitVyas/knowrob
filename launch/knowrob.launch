<launch>
  <arg name="mongodb_name" default="roslog" />
  <arg name="memory_dir" default="" />
  
  <env name="KNOWROB_MONGO_DB_NAME" value="$(arg mongodb_name)" />
  <env name="KNOWROB_MEMORY_DIR" value="$(arg memory_dir)" />
  
  <include file="$(find rosprolog)/launch/rosprolog.launch">
    <arg name="initial_package" default="knowrob" />
    <arg name="initial_goal" default="true" />
  </include>

  <include file="$(find knowrob)/launch/object_state_publisher.launch" />
</launch>
