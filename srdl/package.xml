<?xml version="1.0"?>
<package>
  <name>srdl</name>
  <version>0.1.0</version>
  <description>
    Implementation of the Semantic Robot Description Language (SRDL) that
    provides model and reasoning tools to match prerequisites of actions
    to the capabilities of a robot.
  </description>

  <maintainer email="danielb@uni-bremen.de">Daniel Beßler</maintainer>

  <author>Moritz Tenorth</author>
  <author>Tobias Roehm</author>
  <author email="danielb@uni-bremen.de">Daniel Beßler</author>
 
  <url type="bugtracker">https://github.com/knowrob/knowrob/issues</url>

  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>
  
  <build_depend>knowrob_common</build_depend>
  <build_depend>knowrob_actions</build_depend>
  <build_depend>urdfprolog</build_depend>
  <build_depend>rosprolog</build_depend>
  <build_depend>swi-prolog</build_depend>

  <run_depend>knowrob_common</run_depend>
  <run_depend>knowrob_actions</run_depend>
  <run_depend>urdfprolog</run_depend>
  <run_depend>rosprolog</run_depend>
  <run_depend>swi-prolog</run_depend>

</package>
